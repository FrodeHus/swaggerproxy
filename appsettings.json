{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ProxyConfig": {
    "UpstreamServers": [
      {
        "Name": "EaseeAccessControl",
        "Url": "http://localhost:5020",
        "SwaggerEndpoint": "swagger/v1/swagger.json",
        "Preferred": true
      },
      {
        "Name": "EaseeCloud",
        "Url": "http://localhost:5010",
        "SwaggerEndpoint": "swagger/v1/swagger.json"
      },
      {
        "Name": "ifconfig",
        "Url": "https://ifconfig.me",
        "Routes": [
          {
            "LocalPrefix": "/my_ip",
            "Remote": "/"
          }
        ]
      }
    ]
  }
}
